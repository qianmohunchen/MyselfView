<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="adi.example.bezier">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="adi.example.HostActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="adi.example.path.PathActivity" />
        <activity android:name=".BezierActivity" />
        <activity android:name="adi.example.eventdispatch.EventDispatchActivity" />
        <activity android:name="adi.example.gesturedetector.GestureDetectorActivity" />
        <activity android:name="adi.example.indicator.IndicatorActivity"/>
    </application>

</manifest>